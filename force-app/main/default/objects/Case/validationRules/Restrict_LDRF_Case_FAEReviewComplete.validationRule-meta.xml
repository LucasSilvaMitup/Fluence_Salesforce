<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_LDRF_Case_FAEReviewComplete</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Status,&quot;Complete&quot;),!Is_FAE_Review_complete__c,ISCHANGED(Status), ISPICKVAL( PRIORVALUE(Status) ,&apos;FAE Review&apos;))</errorConditionFormula>
    <errorDisplayField>Is_FAE_Review_complete__c</errorDisplayField>
    <errorMessage>Please select one of the FAE review checkboxes</errorMessage>
</ValidationRule>
