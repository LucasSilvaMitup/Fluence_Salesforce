<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_TS_Case_Closed</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISCHANGED(Status),ISPICKVAL(Status,&quot;Closed&quot;),  $User.Email &lt;&gt; &quot;christopher.bezuyen@fluencebioengineering.com&quot;)</errorConditionFormula>
